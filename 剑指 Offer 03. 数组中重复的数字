class Solution {
public:
    int findRepeatNumber(vector<int>& nums) {
    //时间复杂度n*n
    /*for(int i=0;i<nums.size();i++){
        for(int j=i+1;j<nums.size();j++){
            if(nums[i]==nums[j])
            return nums[i];
        }
    }
    return 0;
  }
};*/
    //时间复杂度nlogn
       sort(nums.begin(), nums.end()); // 对nums进行升序排序
       for(int i=0; i<nums.size(); i++)
       {
            if(nums[i] == nums[i+1])
            return nums[i]; 
        }
        return -1;
    }
};
